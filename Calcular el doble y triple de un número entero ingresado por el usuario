#include <iostream>
using namespace std;

int main() {
    int numero, doble, triple;

    cout << "Ingrese un numero entero: ";
    cin >> numero;

    doble = numero * 2;
    triple = numero * 3;

    cout << "El doble es: " << doble << endl;
    cout << "El triple es: " << triple << endl;

    return 0;
}
