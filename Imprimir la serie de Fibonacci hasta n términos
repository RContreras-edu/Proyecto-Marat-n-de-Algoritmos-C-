#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ingrese la cantidad de tÃ©rminos: ";
    cin >> n;

    int a = 0, b = 1;
    cout << "Serie Fibonacci: ";
    for (int i = 0; i < n; ++i) {
        cout << a << " ";
        int temp = a + b;
        a = b;
        b = temp;
    }
    cout << endl;
    return 0;
}

